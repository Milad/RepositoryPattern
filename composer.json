{
    "name": "miladm/repositorypattern",
    "description": "A small example of Repository Design Pattern",
    "require": {
        "php": ">=7.0",
        "mockery/mockery": "^0.9.9"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.0",
      "squizlabs/PHP_CodeSniffer": "^2.8",
      "phpmd/phpmd" : "@stable",
      "codeclimate/php-test-reporter": "^0.4.4"
    },
    "autoload": {
      "psr-4": {
        "RepositoryPattern\\": "lib/",
        "RepositoryPattern\\Tests\\": "tests/classes/"
      }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "type": "metapackage",
    "license": "MIT",
    "authors": [
      {
        "name": "Milad Kawas Cake",
        "email": "miladkaleh@gmail.com"
      }
    ]
}
